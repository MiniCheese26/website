<aside class=metadata>

  {% if tags %}
    <nav class="taglist">
      <h2>Tags</h2>
      <ul>
        {% for tag in tags -%}
          <li><a class="tag" href="/tags/{{ tag }}">{{ tag }}</a></li> 
        {%- endfor %}
      </ul>
    </nav>
  {% endif %}

  {% if date %}
    <h2>Published</h2>
    <span class="published">{{ date.getFullYear() }}-{{  1+date.getMonth() }}-{{date.getDate()}}</span>
  {% endif %}


</aside>
